<h1>Listing entries for <%= link_to @log.name, log_path(@log) %></h1>

<table>
  <tr>
    <th>Log</th>
    <th>Value</th>
  </tr>

<% @entries.each do |entry| %>
  <tr>
    <td><%=h entry.log_id %></td>
    <td><%=h entry.value %></td>
    <td><%= link_to 'Show', log_entry_path(@log, entry) %></td>
    <td><%= link_to 'Edit', edit_log_entry_path(@log, entry) %></td>
    <td><%= link_to 'Destroy', log_entry_path(@log, entry), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New entry', new_log_entry_path(@log) %>
<p>
  <b>Login:</b>
  <%=h @user.login %>
</p>
 
<p>
  <b>Login count:</b>
  <%=h @user.login_count %>
</p>
 
<p>
  <b>Last request at:</b>
  <%=h @user.last_request_at %>
</p>
 
<p>
  <b>Last login at:</b>
  <%=h @user.last_login_at %>
</p>
 
<p>
  <b>Current login at:</b>
  <%=h @user.current_login_at %>
</p>
 
<p>
  <b>Last login ip:</b>
  <%=h @user.last_login_ip %>
</p>
 
<p>
  <b>Current login ip:</b>
  <%=h @user.current_login_ip %>
</p>

<p>
	<b>Logs:</b>
	<ul>
		<% @user.logs.each do |log| %>
			<li><%= link_to log.name, log_url(log.id) %></li>
		<% end %>
		<li><strong><%= link_to 'Create new log', new_log_url %></strong></li>
	</ul>
</p>
 
 
<%= link_to 'Edit', edit_account_path %>